\begin_layout Standard
Expand the numerator then we have these terms
\begin_inset Formula 
\[
\frac{D_{\phi}^{3}}{D_{\phi}D_{\Lambda}^{4}}\ \frac{(p_{1}.k)^{3}}{D_{\phi}D_{\Lambda}^{4}}\ \frac{D_{\phi}(p_{1}.k)^{2}}{D_{\phi}D_{\Lambda}^{4}}\ \frac{(p_{1}.k)^{2}}{D_{\phi}D_{\Lambda}^{4}}\ \frac{D_{\phi}^{2}(p_{1}.k)}{D_{\phi}D_{\Lambda}^{4}}\ \frac{D_{\phi}(p_{1}.k)}{D_{\phi}D_{\Lambda}^{4}}\ \frac{(p_{1}.k)}{D_{\phi}D_{\Lambda}^{4}}\ \frac{D_{\phi}^{2}}{D_{\phi}D_{\Lambda}^{4}}\ \frac{1}{D_{\phi}D_{\Lambda}^{4}}
\]

\end_inset


\end_layout

\begin_layout Standard
And those terms can be calculated in this way
\begin_inset Formula 
\begin{align*}
\frac{(p_{1}.k)^{3}}{D_{\phi}D_{\Lambda}^{4}} & =0\textrm{due to the odd power in numerator}\\
\frac{D_{\phi}^{3}}{D_{\phi}D_{\Lambda}^{4}} & =\frac{D_{\phi}^{2}}{D_{\Lambda}^{4}}\ \textrm{divergence}\\
\frac{(p_{1}.k)^{2}}{D_{\phi}D_{\Lambda}^{4}} & =\frac{(p_{1}^{+}k^{-})^{2}+(p_{1}^{-}k^{+}-p_{1}^{\perp}.k^{\perp})p_{1}^{+}k^{-}+(p_{1}^{-}k^{+})^{2}+(p_{1}^{\perp}.k^{\perp})^{2}}{D_{\phi}D_{\Lambda}^{4}}\\
 & =\frac{(p_{1}^{+}k^{-})^{2}}{D_{\phi}D_{\Lambda}^{4}}+\frac{p_{1}^{-}k^{+}p_{1}^{+}k^{-}}{D_{\phi}D_{\Lambda}^{4}}-\frac{p_{1}^{\perp}.k^{\perp}p_{1}^{+}k^{-}}{D_{\phi}D_{\Lambda}^{4}}+\frac{(p_{1}^{-}k^{+})^{2}}{D_{\phi}D_{\Lambda}^{4}}+\frac{(p_{1}^{\perp}.k^{\perp})^{2}}{D_{\phi}D_{\Lambda}^{4}}\\
\frac{D_{\phi}(p_{1}.k)^{2}}{D_{\phi}D_{\Lambda}^{4}} & =\frac{(p_{1}^{+}k^{-})^{2}+(p_{1}^{-}k^{+}-p_{1}^{\perp}.k^{\perp})p_{1}^{+}k^{-}+(p_{1}^{-}k^{+})^{2}+(p_{1}^{\perp}.k^{\perp})^{2}}{D_{\Lambda}^{4}}\\
 & =\frac{(p_{1}^{+}k^{-})^{2}}{D_{\Lambda}^{4}}+\frac{p_{1}^{-}k^{+}p_{1}^{+}k^{-}}{D_{\Lambda}^{4}}-\frac{p_{1}^{\perp}.k^{\perp}p_{1}^{+}k^{-}}{D_{\Lambda}^{4}}+\frac{(p_{1}^{-}k^{+})^{2}}{D_{\Lambda}^{4}}+\frac{(p_{1}^{\perp}.k^{\perp})^{2}}{D_{\Lambda}^{4}}\\
\frac{(p_{1}.k)}{D_{\phi}D_{\Lambda}^{4}} & =0\\
\frac{D_{\phi}(p_{1}.k)}{D_{\phi}D_{\Lambda}^{4}} & =\frac{(p_{1}.k)}{D_{\Lambda}^{4}}=0\\
\frac{D_{\phi}^{2}(p_{1}.k)}{D_{\phi}D_{\Lambda}^{4}} & =(D_{\Lambda}+\Lambda^{2}-m_{\phi}^{2})\frac{(p_{1}.k)}{D_{\Lambda}^{4}}\\
 & =\frac{(p_{1}.k)}{D_{\Lambda}^{3}}+(\Lambda^{2}-m_{\phi}^{2})\frac{(p_{1}.k)}{D_{\Lambda}^{4}}\\
\frac{D_{\phi}^{2}}{D_{\phi}D_{\Lambda}^{4}} & =\frac{D_{\phi}}{D_{\Lambda}^{4}}=\frac{(D_{\Lambda}+\Lambda^{2}-m_{\phi}^{2})}{D_{\Lambda}^{4}}=\frac{1}{D_{\Lambda}^{3}}+\frac{\Lambda^{2}-m_{\phi}^{2}}{D_{\Lambda}^{4}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
where there are some terms still need to calculated, how to deal with these
