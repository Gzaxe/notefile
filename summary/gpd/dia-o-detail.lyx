\begin_layout Standard
The second line can be directly calculated.
 In first line, take 
\begin_inset Formula $\frac{D_{B}(p_{1}-k)}{D_{\phi}D_{\Lambda}^{4}D_{B}(p_{2}-k)}$
\end_inset

 as an example
\begin_inset Formula 
\begin{align*}
\frac{D_{B}(p_{1}-k)}{D_{\phi}D_{\Lambda}^{4}D_{B}(p_{2}-k)} & =\frac{D_{B}(p_{2}-k)+2k.q}{D_{\phi}D_{\Lambda}^{4}D_{B}(p_{2}-k)}=\frac{1}{D_{\phi}D_{\Lambda}^{4}}(1+2\frac{k.q}{D_{B}(p_{2}-k)})\\
\frac{1}{D_{\phi}D_{\Lambda}^{4}}\frac{k.q}{D_{B}(p_{2}-k)} & =\frac{1}{D_{\phi}D_{\Lambda}^{4}}\frac{\frac{1}{2}(k^{+}q^{-}+k^{-}q^{+})-k^{\perp}.q^{\perp}}{D_{B}(p_{2}-k)}\\
 & =\frac{1}{D_{\phi}D_{\Lambda}^{4}}\frac{\frac{1}{2}k^{-}q^{+}}{D_{B}(p_{2}-k)}=\frac{1}{D_{\phi}D_{\Lambda}^{4}}\frac{-\xi}{1-\xi}\frac{k^{-}p_{2}^{+}}{D_{B}(p_{2}-k)}\\
 & =\frac{1}{D_{\phi}D_{\Lambda}^{4}}\frac{\xi}{1-\xi}\frac{D_{B}(p_{2}-k)-((p_{2}^{+}-k^{+})p_{2}^{-}+k^{+}k^{-}-(p_{2}^{\perp}-k^{\perp})^{2}-M^{2})}{D_{B}(p_{2}-k)}\\
 & =\frac{\xi}{1-\xi}\frac{1}{D_{\phi}D_{\Lambda}^{4}}
